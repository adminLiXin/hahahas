<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="org.example.mappper.RegisterMapper">
    <select id="login" resultType="Register">
        SELECT * FROM `register` WHERE `userName` = #{userName} and `passWord` = #{passWord}
    </select>

    <insert id="addUser">

    </insert>

    <delete id="delteUser">
        DELETE FROM `register` WHERE `id` = #{id}
    </delete>
</mapper>